    "@types/node-fetch": "^2.6.13",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "prisma": "^6.4.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
